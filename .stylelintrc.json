{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-prettier",
    "stylelint-config-clean-order"
  ],
  "overrides": [
    {
      "files": [
        "**/*.tsx"
      ],
      "customSyntax": "postcss-styled-syntax",
      "rules": {
        "value-keyword-case": null
      }
    }
  ],
  "ignoreFiles": [
    ".yarn/**/*",
    ".next/**/*"
  ]
}